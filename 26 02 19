import random

a = [_ for _ in range(1, 101)]
# random.shuffle(a)
# print(a)
#
# def f(n):
#     for i in range(len(a)):
#         if a[i] == n:
#             return i
# print(f(5))
print(a)
def bin_search(n):
    left = 0
    right = len(a) - 1

    while left <= right:
        mid = (left + right) // 2
        if a[mid] == n:
            return mid
        elif a[mid] < n:
            left = mid + 1
        else:
            right = mid - 1

    return
print(bin_search(15))
